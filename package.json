{
  "name": "@anonaddy/omni-worker",
  "version": "0.0.2-rc1",
  "description": "Run your code including all imports inside workers on either the web or on NodeJS!",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/mjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.cjs.json && tsc -p tsconfig.mjs.json",
    "test:node": "jest --config jest.node.config.ts --detectOpenHandles",
    "test:dom": "jest --config jest.dom.config.ts",
    "prepare": "npm run build"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/avluent/omni-worker"
  },
  "keywords": [
    "worker",
    "multithreading",
    "web worker",
    "worker_threads",
    "omni",
    "typescript"
  ],
  "author": "anonaddy",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.15",
    "bcrypt": "^5.1.1",
    "esbuild-node-externals": "^1.16.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "lodash": "^4.17.21",
    "sqlite3": "^5.1.7",
    "swisseph": "^0.5.17",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@babel/parser": "^7.26.8",
    "@babel/types": "^7.26.8",
    "comlink": "^4.4.2",
    "esbuild": "^0.24.2"
  }
}
