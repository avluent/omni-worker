{
  "name": "@anonaddy/omni-worker",
  "version": "0.0.2",
  "description": "Run your code including all imports inside workers on either the web or on NodeJS!",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/mjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.cjs.json && tsc -p tsconfig.mjs.json",
    "test:node": "NODE_OPTIONS='$NODE_OPTIONS --experimental-vm-modules' jest --config jest.node.config.ts --detectOpenHandles",
    "test:dom": "jest --config jest.dom.config.ts",
    "prepare": "npm run build"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/avluent/omni-worker"
  },
  "keywords": [
    "worker",
    "multithreading",
    "web worker",
    "worker_threads",
    "omni",
    "typescript"
  ],
  "author": "anonaddy",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.15",
    "@types/memory-fs": "^0.3.7",
    "@types/webpack-node-externals": "^3.0.4",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "lodash": "^4.17.21",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@babel/preset-env": "^7.26.8",
    "@babel/preset-typescript": "^7.26.0",
    "babel-loader": "^9.2.1",
    "comlink": "^4.4.2",
    "memory-fs": "^0.5.0",
    "ts-loader": "^9.5.2",
    "webpack": "^5.97.1",
    "webpack-node-externals": "^3.0.0"
  }
}
